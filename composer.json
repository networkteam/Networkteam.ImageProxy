{
  "name": "networkteam/image-proxy",
  "description": "Neos image and thumbnail serving via imgproxy, a fast and scalable microservice for image manipulation ",
  "type": "neos-package",
  "require": {
    "neos/neos": "^9.0",
    "neos/contentgraph-doctrinedbaladapter": "*"
  },
  "authors": [
    {
      "name": "networkteam GmbH",
      "email": "neos@networkteam.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Networkteam\\ImageProxy\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Networkteam\\ImageProxy\\Tests": "Tests"
    }
  },
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit Tests"
  },
  "config": {
    "allow-plugins": {
      "neos/composer-plugin": false
    }
  }
}
